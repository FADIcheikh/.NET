                                    <p>@Html.DisplayFor(model => model.endDateEvent)</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>@Html.DisplayNameFor(model => model.priceEvent)</p>
                                </td>
                                <td>
                                    <p>@Html.DisplayFor(model => model.priceEvent)</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>@Html.DisplayNameFor(model => model.touristicPlace)</p>
                                </td>
                                <td>
                                    <p>@Html.DisplayFor(model => model.touristicPlace.nameTouristicPlace)</p>
                                </td>
                            </tr>
                          

                        </table>

                    </section>





                </div>
                <div>
                    @Html.Partial("../ReservationEvent/suggest")
                </div>
                <script type="text/javascript">
$(function(){
  $('#profiletabs ul li a').on('click', function(e){
    e.preventDefault();
    var newcontent = $(this).attr('href');

    $('#profiletabs ul li a').removeClass('sel');
    $(this).addClass('sel');

    $('#content section').each(function(){
      if(!$(this).hasClass('hidden')) { $(this).addClass('hidden'); }
    });

    $(newcontent).removeClass('hidden');
  });
});
                </script>


            </div>
